{
  "executionRoleArn": "arn:aws:iam::111122223333:role/ECSTaskExecutionRole",
  "containerDefinitions": [{
    "name": "container-demo",
    "image": "111122223333.dkr.ecr.ap-northeast-2.amazonaws.com/flask-demo",
    "essential": true,
    "portMappings": [{
      "hostPort": 80,
      "protocol": "tcp",
      "containerPort": 80
    }]
  }],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "family": "task-demo"
 } 
